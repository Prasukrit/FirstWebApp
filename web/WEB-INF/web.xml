        <servlet-name>ChooseEqServlet</servlet-name>
        <servlet-class>sit.bootcamp.servlet.ChooseEqServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChooseLockerServlet</servlet-name>
        <servlet-class>sit.bootcamp.servlet.ChooseLockerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConServlet</servlet-name>
        <servlet-class>sit.bootcamp.servlet.ConServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VarySimpleServlet</servlet-name>
        <url-pattern>/vss</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SimpleServlet</servlet-name>
        <url-pattern>/simpleservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BankAccountServlet</servlet-name>
        <url-pattern>/BankAccountServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
