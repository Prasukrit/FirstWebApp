    <servlet>
        <servlet-name>SimpleServlet</servlet-name>
        <servlet-class>sit.bootcamp.servlet.SimpleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BankAccountServlet</servlet-name>
        <servlet-class>sit.bootcamp.servlet.BankAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>sit.bootcamp.servlet.Login</servlet-class>
